{
    "name": "starmoozie/laravel-log-manager",
    "description": "An interface to preview, download and delete Laravel log files.",
    "keywords": [
        "starmoozie",
        "log manager",
        "logs",
        "laravel",
        "log interface"
    ],
    "homepage": "https://github.com/starmoozie/laravel-log-manager",
    "license": "MIT",
    "authors": [
        {
            "name": "Starmoozie",
            "email": "starmoozie@gmail.com",
            "role": "Creator & Maintainer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "starmoozie/laravel-crud": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^9.0||^7.0",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Starmoozie\\LaravelLogManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Starmoozie\\LaravelLogManager\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Starmoozie\\LaravelLogManager\\LaravelLogManagerServiceProvider"
            ]
        }
    }
}
